{
  "name": "nextjs-markdown-blog",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "ci": "next lint && npm run typecheck && npm run build",
    "clean-up-branch": "git checkout main && git branch --merged main | grep -v -e main | xargs -I{} git branch -d {}",
    "dev": "npm run format && npm outdated; echo '\r'; next dev",
    "format": "prettier --write .",
    "lint": "next lint",
    "start": "next start",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@mui/material": "^5.14.15",
    "gray-matter": "^4.0.3",
    "next": "14.0.1",
    "next-intercept-stdout": "^1.0.1",
    "react": "^18",
    "react-dom": "^18",
    "react-helmet-async": "^1.3.0",
    "react-markdown": "^9.0.0",
    "react-share": "^4.4.1",
    "recoil": "^0.7.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-syntax-highlighter": "^15.5.9",
    "@vanilla-extract/next-plugin": "^2.3.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.1",
    "prettier": "^2.8.4",
    "react-syntax-highlighter": "^15.5.0",
    "typescript": "^5"
  }
}
