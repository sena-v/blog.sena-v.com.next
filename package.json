{
  "name": "next-markdown-blog",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "build": "rm -r .next; next build",
    "ci": "next lint && npm run typecheck && npm run build",
    "clean-up-branch": "git checkout main && git pull && git branch --merged main | grep -v -e main | xargs -I{} git branch -d {}",
    "dev": "rm -r .next; npm run format && npm outdated; echo '\r'; next dev",
    "e2e:playwright": "npx playwright test",
    "format": "prettier --write --log-level silent . ",
    "lint": "next lint",
    "start": "next start",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@vercel/speed-insights": "^1.1.0",
    "clipboardy": "^4.0.0",
    "gray-matter": "^4.0.3",
    "next": "14.2.29",
    "next-intercept-stdout": "^1.0.1",
    "react": "^19",
    "react-dom": "^19",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.54.0",
    "react-icons": "^5.4.0",
    "react-markdown": "^9.0.1",
    "react-share": "^5.1.1",
    "sharp": "^0.34.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.2.4",
    "@playwright/test": "^1.49.0",
    "@types/gtag.js": "^0.0.20",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vanilla-extract/next-plugin": "^2.4.7",
    "eslint": "^8",
    "eslint-config-next": "14.2.29",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-promise": "^6.4.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-unused-imports": "^4.0.0",
    "prettier": "^3.4.2",
    "react-syntax-highlighter": "^15.6.1",
    "typescript": "^5"
  }
}
