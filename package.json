{
  "name": "nextjs-markdown-blog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "ci": "next lint && npm run typecheck && npm run build",
    "clean-up-branch": "git checkout main && git branch --merged main | grep -v -e main | xargs -I{} git branch -d {}",
    "dev": "npm run format && npm outdated; echo '\r'; next dev",
    "format": "prettier --write .",
    "lint": "next lint",
    "start": "next start",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.8",
    "@netlify/plugin-nextjs": "^4.30.4",
    "gray-matter": "^4.0.3",
    "next": "13.3.4",
    "next-intercept-stdout": "^1.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-markdown": "^8.0.5",
    "react-share": "^4.4.1",
    "recoil": "^0.7.6",
    "remark": "^14.0.2",
    "remark-html": "^15.0.2"
  },
  "devDependencies": {
    "@types/node": "18.16.5",
    "@types/react": "18.2.2",
    "@types/react-dom": "18.2.3",
    "@types/react-syntax-highlighter": "^15.5.6",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.4",
    "prettier": "^2.8.4",
    "react-syntax-highlighter": "^15.5.0",
    "typescript": "4.9.5"
  }
}
