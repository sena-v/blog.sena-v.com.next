{
  "name": "next-markdown-blog",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "ci": "next lint && npm run typecheck && npm run build",
    "clean-up-branch": "git checkout main && git branch --merged main | grep -v -e main | xargs -I{} git branch -d {}",
    "dev": "rm -r .next; npm run format && npm outdated; echo '\r'; next dev",
    "format": "prettier --write --log-level silent . ",
    "lint": "next lint",
    "start": "next start",
    "typecheck": "tsc"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "next": "14.0.4",
    "next-intercept-stdout": "^1.0.1",
    "react": "^18",
    "react-dom": "^18",
    "react-helmet-async": "^2.0.4",
    "react-hook-form": "^7.49.2",
    "react-icons": "^4.12.0",
    "react-markdown": "^9.0.1",
    "react-share": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@vanilla-extract/next-plugin": "^2.3.2",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-n": "^16.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^3.1.1",
    "react-syntax-highlighter": "^15.5.0",
    "typescript": "^5"
  }
}
